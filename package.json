{
  "name": "ng-training",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-gh-pages-directives": "ng build --output-path docs/directives --base-href /ng-training/directives/",
    "build-gh-pages-reactive-forms": "ng build --output-path docs/reactive-forms --base-href /ng-training/reactive-forms/",
    "build-gh-pages-health-app": "ng build --output-path docs/health-app --base-href /ng-training/health-app/",
    "serve-directives": "ng serve --project=directives",
    "serve-reactive-forms": "ng serve --project=reactive-forms",
    "serve-health-app": "ng serve --project=health-app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@types/node": "^12.11.1",
    "typescript": "~4.2.3"
  }
}
